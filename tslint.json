{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb", "tslint-config-prettier"
    ],
    "rules": {
        "no-boolean-literal-compare": false,
        "no-this-assignment": false,
        "import-name": false,
        "variable-name": false,
        "semicolon": [true, "always"],
        "trailing-comma": true
    }
}